{
  "id": "flowchart-process",
  "name": "Process",
  "category": "flowchart",
  "description": "Represents a process or action step in a flowchart",
  "icon": "preview.svg",
  "tags": ["flowchart", "process", "action", "step"],
  
  "defaultSize": {
    "width": 140,
    "height": 80
  },
  
  "defaultStyle": {
    "fill": "#e3f2fd",
    "stroke": "#1976d2",
    "strokeWidth": 2,
    "opacity": 1,
    "textColor": "#000000",
    "fontSize": 14,
    "fontFamily": "Arial, sans-serif",
    "textAlign": "center"
  },
  
  "properties": {
    "cornerRadius": {
      "type": "number",
      "default": 0,
      "min": 0,
      "max": 20,
      "label": "Corner Radius"
    }
  },
  
  "ports": {
    "enabled": true,
    "preset": "standard-4",
    "positions": [
      { "id": "top", "x": 0.5, "y": 0, "type": "input" },
      { "id": "right", "x": 1, "y": 0.5, "type": "output" },
      { "id": "bottom", "x": 0.5, "y": 1, "type": "output" },
      { "id": "left", "x": 0, "y": 0.5, "type": "input" }
    ]
  },
  
  "handles": {
    "enabled": true,
    "preset": "all-corners-sides",
    "positions": ["nw", "n", "ne", "e", "se", "s", "sw", "w"]
  },
  
  "constraints": {
    "minWidth": 60,
    "minHeight": 40,
    "maxWidth": 500,
    "maxHeight": 300,
    "aspectRatio": null
  },
  
  "features": {
    "resizable": true,
    "rotatable": false,
    "connectable": true,
    "groupable": true,
    "lockable": true,
    "clonable": true
  },
  
  "metadata": {
    "version": "1.0.0",
    "author": "System",
    "created": "2025-01-01",
    "updated": "2025-01-01"
  }
}