{
  "id": "flowchart-document",
  "name": "Document",
  "category": "flowchart",
  "description": "Represents a document or report",
  "icon": "preview.svg",
  "tags": ["flowchart", "document", "report", "file"],
  
  "defaultSize": {
    "width": 140,
    "height": 100
  },
  
  "defaultStyle": {
    "fill": "#fce4ec",
    "stroke": "#c2185b",
    "strokeWidth": 2,
    "opacity": 1,
    "textColor": "#000000",
    "fontSize": 14,
    "fontFamily": "Arial, sans-serif",
    "textAlign": "center"
  },
  
  "properties": {
    "waveAmplitude": {
      "type": "number",
      "default": 0.1,
      "min": 0.05,
      "max": 0.2,
      "step": 0.01,
      "label": "Wave Amplitude"
    }
  },
  
  "ports": {
    "enabled": true,
    "preset": "standard-4",
    "positions": [
      { "id": "top", "x": 0.5, "y": 0, "type": "input" },
      { "id": "right", "x": 1, "y": 0.5, "type": "output" },
      { "id": "bottom", "x": 0.5, "y": 0.9, "type": "output" },
      { "id": "left", "x": 0, "y": 0.5, "type": "input" }
    ]
  },
  
  "handles": {
    "enabled": true,
    "preset": "corners-sides",
    "positions": ["nw", "ne", "e", "se", "sw", "w"]
  },
  
  "constraints": {
    "minWidth": 80,
    "minHeight": 60,
    "maxWidth": 400,
    "maxHeight": 300,
    "aspectRatio": null
  },
  
  "features": {
    "resizable": true,
    "rotatable": false,
    "connectable": true,
    "groupable": true,
    "lockable": true,
    "clonable": true
  },
  
  "metadata": {
    "version": "1.0.0",
    "author": "System",
    "created": "2025-01-01",
    "updated": "2025-01-01"
  }
}