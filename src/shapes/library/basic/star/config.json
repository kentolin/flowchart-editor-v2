{
  "id": "star",
  "name": "Star",
  "category": "basic",
  "description": "Star shape with configurable points",
  "defaults": {
    "width": 100,
    "height": 100,
    "points": 5,
    "innerRadius": 0.4
  },
  "properties": {
    "points": {
      "type": "number",
      "label": "Points",
      "min": 3,
      "max": 12,
      "default": 5
    },
    "innerRadius": {
      "type": "number",
      "label": "Inner Radius",
      "min": 0.2,
      "max": 0.8,
      "step": 0.1,
      "default": 0.4
    }
  },
  "style": {
    "fill": "#ffffff",
    "stroke": "#000000",
    "strokeWidth": 2
  }
}